<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.marcelveldt" version="1.0.0" name="Marcelveldt's BETA repository" provider-name="marcelveldt">

	<extension point="xbmc.addon.repository" name="Marcelveldt's BETA repository">
		<info compressed="false">https://raw.githubusercontent.com/marcelveldt/repo.marcelveldt/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/marcelveldt/repo.marcelveldt/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/marcelveldt/repo.marcelveldt/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Kodi repository for beta/test versions of Marcelveldt's addons</summary>
		<description lang="en">Kodi repository for beta/test versions of Marcelveldt's addons, for stable versions please just use the official Kodi repository. This repo is for test/beta versions of my work only</description>
		<platform>all</platform>
	</extension>
</addon>

<addon
	id="skin.titan.jarvisbeta"
	version="3.6.89"
	name="Titan BETA (Jarvis)"
	provider-name="marcelveldt">
	<requires>
		<import version="5.10.0" addon="xbmc.gui"/>
		<import addon="script.skinshortcuts" version="0.6.5"/>
		<import addon="script.skin.helper.service" version="1.0.0"/>
        <import addon="resource.uisounds.titan.modern" version="1.0.0"/>
        <import addon="resource.images.weathericons.outline" version="0.0.4" optional="true"/>
        <import addon="resource.images.weatherfanart.single" version="0.0.4" optional="true"/>
	</requires>
	<extension
    point="xbmc.gui.skin"
    defaultthemename="Textures.xbt"
    debugging="false">
		<res width="1920" height="1080" aspect="16:9" default="true" folder="1080i" />
		<res width="1920" height="1200" aspect="16:10" default="false" folder="1080i" />
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="de">MediaPortal's Titan Skin portiert für Kodi mit jeder Menge Funktionen.</summary>
		<summary lang="en">MediaPortal's Titan skin ported to Kodi enriched with lot's of features.</summary>
		<summary lang="nl">De Titanskin van MediaPortal is geconverteerd naar Kodi en verrijkt met talloze functionaliteiten</summary>
		<platform>all</platform>
		<license>GPL v2.0</license>
		<forum>http://forum.kodi.tv/forumdisplay.php?fid=212</forum>
		<website>http://forum.kodi.tv/forumdisplay.php?fid=212</website>
		<email></email>
		<source></source>
	</extension>
</addon>

